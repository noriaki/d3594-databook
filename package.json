    "completion": "node completion.js",
    "merge": "node merge.js",
    "export": "node export.js",
    "sync": "run-s sync:datas sync:images",
    "sync:datas": "node sync-spreadsheets.js",
    "sync:images": "node upload-images-s3.js",
    "start": "run-s -n clean crawl images completion merge export"
